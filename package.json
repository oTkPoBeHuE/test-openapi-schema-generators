{
  "name": "test-openapi-schema-generators",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint my-vue-app",
    "preview": "vite preview",
    "clean": "rimraf --glob \"**/__generated__\" \"!node_modules/**/*\"",
    "download-schema": "node ./src/app/downloadSchema/downloadSchema.js && prettier --write openapi.json",
    "orval:run-config": "orval --config src/api/orval/multiple-config/orval.config.ts",
    "orval:generate": "node src/api/orval/run.js",
    "openapi-ts:run-config": "openapi-ts -f src/api/hey-api_openapi-ts/base/openapi-ts.config.ts",
    "openapi-ts:generate": "node src/api/hey-api_openapi-ts/run.js",
    "openapi-typescript:run-config": "node --trace-deprecation npx openapi-typescript src/api/schemas/petstore/petstore.yaml -o src/api/openapi-typescript/v1.d.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^9.3.0",
    "@hey-api/client-fetch": "^0.5.2",
    "@tanstack/react-query": "^5.62.3",
    "@tanstack/react-query-devtools": "^5.62.3",
    "axios": "^1.7.9",
    "msw": "^2.6.8",
    "openapi-fetch": "^0.13.3",
    "openapi-react-query": "^0.2.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@hey-api/openapi-ts": "^0.59.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "openapi-typescript": "^7.4.4",
    "orval": "^7.3.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1"
  }
}
